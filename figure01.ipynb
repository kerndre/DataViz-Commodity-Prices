   "source": [
    "# Diagramm 1\n",
    "# Dieses Diagramm besteht aus zwei Teilen:\n",
    "# 1. Ein Graph mit den summierten Preisen aller Waren über den gesamten Zeitraum 2000-2022\n",
    "# 2. Ein Graph mit den Preisen für alle Waren aufgeteilt nach diesen, ebenfalls über den gesamten Zeitraum 2000-2022\n",
    "# Außerdem wurde für den zweiten Graphen ein graphischer Highlighter eingefügt\n",
    "\n",
    "# Plot mit summierten Preisen aller Waren über den gesamten Zeitraum (2000-2022)\n",
    "fig_all_sum = alt.Chart(\n",
    "    df\n",
    ").mark_line(\n",
    "    tooltip=True,\n",
    "    color='black'\n",
    ").encode(\n",
    "    x=alt.X('date:T',\n",
    "            axis=alt.Axis(title='Jahr')),\n",
    "    y=alt.Y('mean_category:Q',\n",
    "            aggregate='sum',\n",
    "            axis=alt.Axis(title='Rohstoffpreis (€), Durchschnitt'))\n",
    ").properties(title='Rohstoffpreise 2000-2022, summiert', width=1600, height=400)\n",
    "\n",
    "# Plot mit Preisen nach Waren aufgeteilt über den gesamten Zeitraum\n",
    "fig_all_commodities = alt.Chart(\n",
    "    df\n",
    ").encode(\n",
    "    x=alt.X('date:T',\n",
    "            axis=alt.Axis(title='Jahr')),\n",
    "    y=alt.Y('mean_category:Q',\n",
    "            axis=alt.Axis(title='Rohstoffpreis (€), Durchschnitt')),\n",
    "    color=alt.Color('category:N',\n",
    "                    scale=alt.Scale(domain=domain, range=range_),\n",
    "                    sort=alt.EncodingSortField('mean_category', op='mean', order='descending'),\n",
    "                    title='Rohstoffe'\n",
    "    )\n",
    ").properties(title='Rohstoffpreise 2000-2022, kategorisiert',width=1600, height=400)\n",
    "\n",
    "# Graphischer Highlighter der Waren\n",
    "highlight = alt.selection(type='single', \n",
    "                          on='mouseover', \n",
    "                          fields=['category'],\n",
    "                          nearest=True)\n",
    "\n",
    "points = fig_all_commodities.mark_circle(\n",
    "    tooltip=True\n",
    ").encode(\n",
    "    opacity=alt.value(0)\n",
    ").add_selection(highlight)\n",
    "\n",
    "lines = fig_all_commodities.mark_line(\n",
    "    tooltip=True\n",
    "    ).encode(\n",
    "    size=alt.condition(~highlight, alt.value(1.5), alt.value(3.5))\n",
    ").transform_loess('date', 'mean_category',bandwidth=.005, groupby=['category']).mark_line(size=3)\n",
    "\n",
    "# Kombination von Plot und Highlighter\n",
    "fig_commodities_highlight = (points + lines)\n",
    "\n",
    "# Verknüpfung beider Plots zu Diagramm 1 mit eine LOESS-Linie\n",
    "fig_1 = (fig_all_sum.transform_loess('date', 'mean_category',bandwidth=.005, groupby=['category']).mark_line(size=3, tooltip=True, color='black') & fig_commodities_highlight)\n",
    "\n",
    "# Quelle Highlighter: https://altair-viz.github.io/gallery/multiline_highlight.html"
   ]
  },
